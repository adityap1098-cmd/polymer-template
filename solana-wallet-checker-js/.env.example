# Solana Wallet Checker Bot - Environment Configuration
# Copy this file to .env and fill in your values

# Solana RPC endpoint (QuickNode recommended for production)
# Public RPC has strict rate limits (~100 requests/10 seconds)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WSS_URL=wss://api.mainnet-beta.solana.com

# Wallet classification threshold
# Number of unique tokens to classify as "OLD" wallet
OLD_WALLET_THRESHOLD=5

# Polling interval in seconds (for polling mode)
POLL_INTERVAL=5
